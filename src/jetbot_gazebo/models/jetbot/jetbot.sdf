<?xml version="1.0" ?>
<sdf version="1.8">
  <model name='jetbot' canonical_link='base_link'>

    <!-- Hlavné telo robota -->
    <link name='base_link'>
      <pose relative_to='__model__'>0 0 0.005 0 0 0</pose>
      <inertial>
        <mass>1.6</mass>
        <inertia>
          <ixx>0.0055</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>0.0055</iyy>
          <iyz>0</iyz>
          <izz>0.0091</izz>
        </inertia>
      </inertial>
      <visual name='base_link_visual'>
        <geometry>
          <cylinder>
            <radius>0.1066</radius>
            <length>0.05</length>
          </cylinder>
        </geometry>
        <material>
          <ambient>0.0 1.0 0.0 1</ambient>
          <diffuse>0.0 1.0 0.0 1</diffuse>
          <specular>0.0 1.0 0.0 1</specular>
        </material>
      </visual>
      <collision name='base_link_collision'>
        <geometry>
          <cylinder>
            <radius>0.1066</radius>
            <length>0.05</length>
          </cylinder>
        </geometry>
      </collision>

      <!-- Sensor pre IMU -->
      <sensor name="imu_sensor" type="imu">
        <update_rate>100</update_rate>
        <always_on>1</always_on>
        <visualize>true</visualize>
        <topic>/imu</topic>
      </sensor>

      <!-- Sensor pre kontaktný senzor -->
      <sensor name="contact_sensor" type="contact">
        <update_rate>100</update_rate>
        <contact>
          <collision>base_link_collision</collision>
          <collision>left_wheel_collision</collision>
          <collision>right_wheel_collision</collision>
        </contact>
      </sensor>
    </link>

    <!-- Ľavé koleso -->
    <link name='left_wheel'>
      <pose relative_to="base_link">0 0.1066 -0.02 1.5707 0 0</pose>
      <inertial>
        <mass>0.1</mass>
        <inertia>
          <ixx>0.000036</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>0.000054</iyy>
